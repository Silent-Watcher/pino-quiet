{
	"name": "pino-quiet",
	"version": "0.9.0",
	"description": "A lightweight pino transport that collapses repeated consecutive log messages.",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"engines": {
		"node": ">=v22.18.0"
	},
	"exports": {
		".": {
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"keywords": [
		"pino",
		"transport",
		"log",
		"deduplicate",
		"noise",
		"quiet"
	],
	"repository": {
		"url": "https://github.com/Silent-Watcher/pino-quiet"
	},
	"author": {
		"email": "backendwithali@gmail.com",
		"name": "Ali nazari"
	},
	"scripts": {
		"tsc": "tsc",
		"dev": "node --watch src/index.ts",
		"prebuild": "npm run tsc && npm run clean",
		"build": "tsup",
		"prestart": "npm run build",
		"start": "node dist/index.js",
		"test": "vitest run --coverage",
		"test:watch": "vitest dev",
		"prelint": "biome format --write ./src/*.ts ./tests/*.ts",
		"lint": "biome lint ./src/*.ts ./tests/*.ts",
		"check": "biome check --write ./src/*.ts ./tests/*.ts",
		"prepare": "husky",
		"commit": "cz",
		"clean": "del-cli dist/ coverage/ *.log",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"license": "MIT",
	"type": "module",
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"cz-conventional-changelog": "^3.3.0",
		"del-cli": "^7.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"pino": "^10.1.0",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.14"
	},
	"peerDependencies": {
		"pino": "^10.1.0"
	},
	"dependencies": {
		"fast-deep-equal": "^3.1.3",
		"pino-abstract-transport": "^3.0.0",
		"sonic-boom": "^4.2.0"
	}
}
